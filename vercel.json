{
  "version": 2,
  "rewrites": [
    { "source": "/", "destination": "/frontend/index.html" },
    { "source": "/arena", "destination": "/frontend/arena.html" },
    { "source": "/agent", "destination": "/frontend/agent.html" },
    { "source": "/dashboard", "destination": "/frontend/dashboard.html" },
    { "source": "/SKILL.md", "destination": "/frontend/SKILL.md" },
    { "source": "/og.jpg", "destination": "/frontend/og.jpg" },
    { "source": "/(.*)", "destination": "/frontend/$1" }
  ]
}
